query Test($type: NodeContainerType, $invType: NodeContainerType) {

  # Via UUID
  nodePerUuidInverted: node(uuid: "02e3982b6ad14c1fa4a0b5998a02ff92", nodeType: $invType) {

    # NoLang
    nolang: node (lang: "ru", nodeType: $type) {
      uuid
      version
      isPublished
      isDraft
    }
  }
}

