{
  "type" : "object",
  "id" : "urn:jsonschema:com:gentics:mesh:core:rest:release:info:ReleaseInfoSchemaList",
  "properties" : {
    "schemas" : {
      "type" : "array",
      "description" : "List of schema references.",
      "items" : {
        "type" : "object",
        "id" : "urn:jsonschema:com:gentics:mesh:core:rest:release:info:ReleaseSchemaInfo",
        "properties" : {
          "name" : {
            "type" : "string",
            "description" : "Name of the referenced element"
          },
          "uuid" : {
            "type" : "string",
            "required" : true,
            "description" : "Uuid of the referenced element"
          },
          "version" : {
            "type" : "string",
            "required" : true,
            "description" : "The version of the microschema."
          },
          "migrationStatus" : {
            "type" : "string",
            "description" : "Status of the migration which was triggered when the schema/microschema was added to the release.",
            "enum" : [ "IDLE", "UNKNOWN", "QUEUED", "STARTING", "RUNNING", "COMPLETED", "FAILED" ]
          },
          "jobUuid" : {
            "type" : "string",
            "description" : "Uuid of the migration job."
          }
        }
      }
    }
  }
}